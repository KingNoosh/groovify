{
  "manifest_version": 2,
  "name": "Groovify",
  "description": "This extension groovifies the current page",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "https://facebook.com/"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "css" : ["fb.css"]
    }
  ]
}
